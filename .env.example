# AI Browser Automation API - Example Configuration

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Browser Configuration
BROWSER_TYPE=chromium
HEADLESS=False  # Set to True for headless mode (not recommended for production)

# File Storage
OUTPUT_DIR=./output
SESSION_DIR=./sessions

# AI Website URLs
GROK_URL=https://grok.ai
X_AI_URL=https://x.ai

# OAuth Configuration (optional - required for OAuth login)
# Register your application with each OAuth provider to get these credentials
OAUTH_GOOGLE_CLIENT_ID="your_google_client_id.apps.googleusercontent.com"
OAUTH_GOOGLE_CLIENT_SECRET="your_google_client_secret"
OAUTH_GITHUB_CLIENT_ID="your_github_client_id"
OAUTH_GITHUB_CLIENT_SECRET="your_github_client_secret"
OAUTH_TWITTER_CLIENT_ID="your_twitter_client_id"
OAUTH_TWITTER_CLIENT_SECRET="your_twitter_client_secret"

# Timeout settings (in seconds)
GENERATION_TIMEOUT=300  # 5 minutes for generation
LOGIN_TIMEOUT=120  # 2 minutes for login